{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/static/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/static/index.html"
    },
    {
      "src": "/api/*",
      "dest": "/api/app.py"
    }
  ]
}
